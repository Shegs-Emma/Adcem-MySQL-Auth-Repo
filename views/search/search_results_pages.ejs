<% include ../partials/header %>

<div class="container ser-div">
	<table class="table table-hover" id="tbl">
		<tr id="table-head">
			<th>ID</th>
			<th>HOSPITAL NAME</th>
			<th>REGION</th>
		</tr>
		<% hospitals.forEach(function(hospital){ %>
		<tr id="table-data">
			<td class="id"><a href="#"> <%= hospital.id %> </a></td>
			<td class="hosp_name"><a href="#" id="test"> <%= hospital.hospital_name %> </a></td>
			<td class="region"><a href="#"> <%= hospital.region %> </a></td>
		</tr>
		<% }) %>
	</table>

<a href="/search" class="btn btn-warning">Back</a>
</div>
<% include ../partials/footer %>